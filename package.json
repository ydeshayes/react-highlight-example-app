{
  "name": "Power",
  "version": "1.0.0",
  "description": "Highlightable demo page source",
  "scripts": {
    "start-message": "babel-node tools/startMessage.js",
    "prestart": "npm-run-all --parallel start-message remove-dist lint:tools",
    "start": "npm-run-all --parallel open:src",
    "start:e2e": "npm run build:test; babel-node tools/testServer.js",
    "open:src": "babel-node tools/srcServer.js",
    "open:dist": "babel-node tools/distServer.js",
    "open:test": "babel-node tools/distServer.js",
    "lint:tools": "eslint webpack.config.js tools",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "node_modules/.bin/rimraf ./dist",
    "clean-dist:test": "npm run remove-dist:test && mkdir dist_test",
    "remove-dist:test": "node_modules/.bin/rimraf ./dist_test",
    "build:html": "NODE_ENV='production' babel-node tools/buildHtml.js",
    "build:test": "npm run clean-dist:test; NODE_ENV='test' babel-node tools/buildHtml.js; babel-node tools/buildTest.js",
    "prebuild": "npm run clean-dist && npm run build:html",
    "build": "babel-node tools/build.js",
    "build:staging": "npm run clean-dist; NODE_ENV='staging' babel-node tools/buildHtml.js;NODE_ENV='staging' babel-node tools/build.js",
    "test": "mocha tools/testSetup.js src/**/**/*.spec.js",
    "test:watch": "npm run test -- --watch"
  },
  "dependencies": {
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-runtime": "6.25.0",
    "classnames": "2.2.5",
    "flexboxgrid": "6.3.1",
    "highlightable": "1.1.0-beta.0",
    "immutable": "3.8.1",
    "js-md5": "0.6.0",
    "material-ui": "0.18.7",
    "object-assign": "4.1.1",
    "rc-tooltip": "3.4.7",
    "react": "15.6.1",
    "react-dom": "15.6.1",
    "react-immutable-proptypes": "2.1.0",
    "react-redux": "5.0.5",
    "react-tap-event-plugin": "2.0.1",
    "redux": "3.7.2",
    "redux-thunk": "2.2.0",
    "substyle": "6.0.0"
  },
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-core": "6.25.0",
    "babel-eslint": "7.2.3",
    "babel-loader": "7.1.1",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-1": "6.24.1",
    "babel-register": "6.24.1",
    "browser-sync": "2.18.13",
    "cheerio": "0.22.0",
    "colors": "1.1.2",
    "connect-history-api-fallback": "1.3.0",
    "cross-env": "5.0.1",
    "css-loader": "0.28.4",
    "eslint": "4.3.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-react": "7.1.0",
    "extract-text-webpack-plugin": "3.0.0",
    "file-loader": "0.11.2",
    "json-loader": "0.5.7",
    "node-sass": "5.0.0",
    "npm-run-all": "4.0.2",
    "react-addons-test-utils": "15.6.0",
    "rimraf": "2.6.1",
    "sass-loader": "6.0.6",
    "style-loader": "0.18.2",
    "webpack": "3.4.1",
    "webpack-dev-middleware": "1.11.0",
    "webpack-hot-middleware": "2.18.2"
  }
}
